    "description": "Symfony ConfigCacheBundle for easier handling of user-defined configuration file cache",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.7",
        "symfony/config": "~2.7",
        "symfony/console": "~2.7",
        "symfony/dependency-injection": "~2.7",
        "symfony/finder": "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/translation": "~2.7",
        "symfony/yaml": "~2.7",
        "doctrine/cache": "~1.3"
    },
    "require-dev": {
